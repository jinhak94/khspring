<!-- Navigator view의 target/classes/mapper/**/*.xml -->

	<select id="selectMemoList" resultType="memo">
		select
			*
		from
			memo
	</select>
	<insert id="insertMemo">
		insert into
			memo
		values(
			seq_memo_no.nextval,
			#{memo},
			#{password},
			default
		)
	</insert>
	<resultMap type="member" id="memberMap">
